BINDIR = {PACKAGEPREFIX}{BINDIR}
UADE123NAME={UADE123NAME}

CC = {CC}
CFLAGS = -Wall -O2 -pthread `pkg-config fuse --cflags` -DUADENAME=\"{BINDIR}/{UADE123NAME}\" {DEBUGFLAGS} {ARCHFLAGS}
CLIBS = {ARCHLIBS} `pkg-config fuse --libs`
MODULES = uadefs.o

%.o:	%.c
	$(CC) $(CFLAGS) -c $<

all:	uadefs

uadefs:	$(MODULES)
	$(CC) $(CFLAGS) $(CLIBS) -o $@ $(MODULES)

uadefs.o:	uadefs.c

clean:	
	rm -f uadefs *.o
